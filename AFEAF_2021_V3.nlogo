to init-oppida
  ask Oppida with [age = 0] [
    set shape "House"
    set color white
    set size 0.5
    set Age 0
    set Stock 0
    set N_inhabitants population_density * 64
    set Workforce N_inhabitants / (100 / Workforce%); Calcule le % de main-d'oeuvre/population
    set Area_cultivated Workforce * Area/person
    set Production Area_cultivated * WR3
    set Necessary_food N_inhabitants * Wheat/Person / 1000; chaque individu consomme 200-400 kg de blé/an (recalculé en tonnes)
    ;set Max_exploited_area 3.14 * 5 * 5 * 100 / 3 / 2 ;superficie dans un rayon de 5 km autour de l'établissement, mais seulement 1/3 exploitable + jachère d'1 an sur 2
    set Max_exploited_area 8 * 8 * 100 / 2; superficie maximum (HA) pouvant être exploitée dans une cellule de 8 x 8 km avec une jachère d'1 an/2
    set Seeds Area_cultivated * 0.13
    set Surplus2 []
    ;if Area_cultivated > Max_exploited_area [set N_inhabitants Max_exploited_area * WR / 0.2]
    ;if Production < Necessary_food [die]
  ]
end

