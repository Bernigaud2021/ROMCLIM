to emulate-LPJML_var
  ask patches [
      set WR 0.3452 + 0.033 * Thiv_var + 0.0072 * Tprt_var + 0.0467 * Tete_var - 0.0523 * Taut_var +  0.0016 * Pprt_var + 1e-04 * Pete_var - 4e-04 * Paut_var; rendement blé pluvial (wheath rainfed)
     if WR = 0.3452 or WR < 0 [set WR 0]
  ]
end

to color-patches
  ask patches [
  set pcolor scale-color yellow WR 1.5 0
  if WR = 0 and Pycor < 19 [set pcolor blue]
  if WR = 0 and Pxcor > 54 and Pycor > 19 [set pcolor 9]
  if WR = 0 and Pxcor < 13 and Pycor < 3 [set pcolor 9]
  ]
end

to create_var
ask patches [
  set Thiv_var Thiv * VAR_ + Thiv_ano
  set Tprt_var Tprt * VAR_ + Tprt_ano
  set Tete_var Tete * VAR_ + Tete_ano
  set Taut_var Taut * VAR_ + Taut_ano
  set Phiv_var Phiv * VAR_ + Phiv_ano
  set Pprt_var Pprt * VAR_ + Pprt_ano
  set Pete_var Pete * VAR_ + Pete_ano
  set Paut_var Paut * VAR_ + Paut_ano
  ]
end

