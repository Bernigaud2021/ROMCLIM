to go-oppida
  ;create-new-oppida
  init-oppida
  ask Oppida [
    set Age Age + 1
    ;set Surplus2 []
    ;set N_inhabitants N_inhabitants * (1 + Growth_rate / 100) ;annual growth of population = 2 %
    set N_inhabitants N_inhabitants + (N_inhabitants / 100 * Natality_rate) - (N_inhabitants / 100 * Mortality_rate)
    set Workforce N_inhabitants / (100 / Workforce%); Calcule le % de main-d'oeuvre/population
    ;set Workforce N_inhabitants / 3; 1/3 des habitants peuvent travailler
    set Area_cultivated Workforce * Area/person; chaque individu de la workforce peut cultiver jusqu'à 2 ha seul
    set Seeds Area_cultivated * 0.13 ;130 kg de semences à l'hectare (5 modius/jugère de Columelle)
    set Production Area_cultivated * WR3
    ;set Necessary_food N_inhabitants * 0.2; chaque individu consomme 200 kg de blé/an
    set Necessary_food N_inhabitants * Wheat/Person / 1000; chaque individu consomme entre 200 et 500 kg de blé/an
    ;set Max_exploited_area 3.14 * 5 * 5 * 100 ;maximum de la superficie exploitable = rayon de 5 km autour de l'établissement
    set Max_exploited_area 8 * 8 * 100 / 2; superficie maximum (HA) pouvant être exploitée dans une cellule de 8 x 8 km avec une jachère d'1 an/2
    set Surplus Production - Necessary_food - Seeds
    if Surplus < 0 [set Surplus 0]
    ;set Surplus2 lput Surplus Surplus2
    ;if ticks > 2 [set Surplus2 remove-item 0 Surplus2]
    ;set Stock sum Surplus2
    ;if Area_cultivated > Max_exploited_area [set N_inhabitants Max_exploited_area * WR3 / 0.2]; revoir ici pour remplacer 0.2 par une variable
    ;if Area_cultivated > Max_exploited_area [set N_inhabitants N_inhabitants]
    if Area_cultivated > Max_exploited_area [set natality_rate 0]
    ;if Area_cultivated > Max_exploited_area [set N_inhabitants 100]
    if Necessary_food > Production and Necessary_food > Stock [die]
    ;set size log 10 N_inhabitants
    set size 0.5
    ;set size Age / 600
  ]
end

to-report mean-WR
