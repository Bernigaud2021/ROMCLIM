to go
  create_var
  compute-climatic-data
  emulate-LPJML_var
  color-patches
  ;if Culture = "Wheat rainfed" [
    ;ask Farms [die]
    ;setup-Farms-R
    ;ask Farms [if harvest < people * 0.3 [die]]; les fermes meurent si la quantité de blé pour la nourriture des exploitants n'est pas produite dans l'année (0.3 tonnes par habitants/par an)
  ;]
  create-new-Oppida
  go-Oppida
  if ticks > 0 [compute-climatic-data-2500BP]
  if ticks > 100 [compute-climatic-data-2400BP]
  if ticks > 200 [compute-climatic-data-2300BP]
  if ticks > 300 [compute-climatic-data-2200BP]
  if ticks > 400 [compute-climatic-data-2100BP]
  if ticks > 500 [compute-climatic-data-2000BP]
  if ticks = 600 [stop]
  set Date Date - 1
  ask patches [set WR2 WR * random-float 1.5]
  ask patches [
    set WR3 WR * %_yields / 100
    ;set WR3 random-float (WR3 * 1.5 - WR3 * 0.5) + WR3 * 0.5 ; fait varier aléatoirement de 20 % de part et d'autre de la moyenne
  ]
  set VAR_ 0.9 + random-float 0.2
  ;ask oppida [if Production < Necessary_food [die]]
  ;ask patches [if not any? oppida-here and WR3 > 0 [sprout-oppida 1]]
